   "source": [
    "n, m = map(int,input().split())\n",
    "\n",
    "coin =[int(i) for i in input().split()]\n",
    "\n",
    "dp =[50000]*(n+1)\n",
    "dp[0]=0\n",
    "\n",
    "for i in range(50000):\n",
    "  for j in coin:\n",
    "    if i+j<=len(dp)-1:\n",
    "      dp[i+j] =min(dp[i+j],dp[i]+1)\n",
    "     \n",
    "    else:\n",
    "      continue\n",
    "\n",
    "print(dp[n])"
   ]
